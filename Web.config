<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <rewrite>
		  <rules>
		    <rule name="Rewrite to registrar turno">
		      <match url="^clientes/registrar-turno" />
		      <action type="Rewrite" url="index.php?action=registrar-turno" />
		    </rule>
		    <rule name="Rewrite to solicitar plan">
		      <match url="^clientes/solicitar-plan" />
		      <action type="Rewrite" url="index.php?action=solicitar-plan" />
		    </rule>
		    <rule name="Rewrite to planificar plan">
		      <match url="^clientes/planificar-plan" />
		      <action type="Rewrite" url="index.php?action=planificar-plan" />
		    </rule>
		    <rule name="Rewrite to modificar plan">
		      <match url="^clientes/modificar-plan" />
		      <action type="Rewrite" url="index.php?action=modificar-plan" />
		    </rule>
		    <rule name="Rewrite to ver turnos">
		      <match url="^clientes/mis-turnos" />
		      <action type="Rewrite" url="index.php?action=mis-turnos" />
		    </rule>
		    <rule name="Rewrite to tipos de lavado">
		      <match url="^tipos-de-lavado" />
		      <action type="Rewrite" url="index.php?action=tipos-de-lavado" />
		    </rule>
		    <rule name="Rewrite to agregar tipos de lavado">
		      <match url="^agregar-tipo-de-lavado" />
		      <action type="Rewrite" url="index.php?action=agregar-tipo-de-lavado" />
		    </rule>
		    <rule name="Rewrite to editar tipos de lavado">
		      <match url="^editar-tipo-de-lavado" />
		      <action type="Rewrite" url="index.php?action=editar-tipo-de-lavado" />
		    </rule>
		    <rule name="Rewrite to lista empleados">
		      <match url="^lista-empleados" />
		      <action type="Rewrite" url="index.php?action=lista-empleados" />
		    </rule>
		    <rule name="Rewrite to agregar empleado">
		      <match url="^agregar-empleado" />
		      <action type="Rewrite" url="index.php?action=agregar-empleado" />
		    </rule>
		    <rule name="Rewrite to editar empleado">
		      <match url="^editar-empleado" />
		      <action type="Rewrite" url="index.php?action=editar-empleado" />
		    </rule>
		    <rule name="Rewrite to login.php">
		      <match url="^iniciar-sesion" />
		      <action type="Rewrite" url="login.php" />
		    </rule>
		    <rule name="Rewrite to register">
		      <match url="^registro" />
		      <action type="Rewrite" url="register.php" />
		    </rule>
		    <rule name="Rewrite to password reset">
		      <match url="^recuperar-contrasena" />
		      <action type="Rewrite" url="password_reset.php" />
		    </rule>
		  </rules>
		</rewrite>
    </system.webServer>
</configuration>